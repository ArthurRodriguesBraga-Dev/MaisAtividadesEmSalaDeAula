double nota;
Console.WriteLine("Este programa irá ler a nota do aluno");
Console.Write("Digite a nota do aluno: ");
nota = Convert.ToDouble(Console.ReadLine());
if (nota >= 9)
{
    Console.WriteLine($"Sua nota é {nota} e ela é excelente!");
}
else if (nota < 9 && nota >= 7)
{
    Console.WriteLine($"Sua nota é {nota} e ela é boa!");   
}
else if (nota < 7 && nota >= 5)
{
    Console.WriteLine($"Sua nota é {nota} e ela é regular!");
}
else if (nota < 5)
{
    Console.WriteLine($"Sua nota é {nota} e ela é insuficiente!");
}
else
{
    Console.WriteLine("Algo deu errado. Tente novamente.");
}
Console.ReadKey();
